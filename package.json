{
  "name": "@polvo-labs/form-utils",
  "version": "3.0.3",
  "description": "Form helpers for data validation and formatting",
  "files": [
    "dist"
  ],
  "source": "./src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "keywords": [
    "form",
    "final-form",
    "validators",
    "formatters",
    "mask",
    "parsers"
  ],
  "scripts": {
    "test": "jest",
    "build": "microbundle",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "is-valid-birthdate": "^0.1.0",
    "msk": "^1.0.3"
  },
  "devDependencies": {
    "babel-jest": "^29.1.2",
    "jest": "^29.1.2",
    "microbundle": "^0.15.1"
  },
  "author": "Guilherme Santiago",
  "homepage": "https://github.com/polvo-labs/form-utils",
  "license": "MIT",
  "repository": "https://github.com/polvo-labs/form-utils",
  "jest": {
    "setupFiles": [
      "./tests/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/*"
    ],
    "verbose": true,
    "clearMocks": true,
    "resetMocks": true,
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    }
  }
}
